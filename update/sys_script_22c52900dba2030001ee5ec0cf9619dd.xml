	var wrknumber = current.workshop;
	var wrkusr = current.attendee;
	var gr = new GlideRecord('x_34314_wrkshp_reg_workshop_attendees');
	gr.addQuery('attendee',wrkusr);
	gr.addQuery('workshop',wrknumber);
	gr.query();
	
	if(gr.next()){
		//User already registered
		var url = "x_34314_wrkshp_reg_workshops.do?sys_id=" + wrknumber;
		gs.addInfoMessage("Return to <a href='" + url + "'>" + current.workshop.number + "</a>");
		gs.addErrorMessage('User is already registered for this workshop');
		current.setAbortAction(true);

	}

})(current, previous);]]></script>
